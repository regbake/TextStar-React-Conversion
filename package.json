  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "ejs": "^2.5.8",
    "express": "^4.16.2",
    "file-system": "^2.2.2",
    "multer": "^1.3.0",
    "path": "^0.12.7"
  }
}
